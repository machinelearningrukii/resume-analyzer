{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/analyze",
      "destination": "api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/app.tsx"
    }
  ]
}